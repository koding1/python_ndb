from collections import deque

# deque(데크) = double-ended queue
# 양 방향에서 데이터 처리가 가능한 큐 자료구조를 의미
queue = deque()

queue.append(5)
queue.append(2)
queue.append(3)
queue.append(7)
queue.popleft()
queue.append(1)
queue.append(4)
queue.popleft()

# 먼저 들어온 순서대로 출력
print(queue)

# 역순으로 바꾸기
queue.reverse()
print(queue)
